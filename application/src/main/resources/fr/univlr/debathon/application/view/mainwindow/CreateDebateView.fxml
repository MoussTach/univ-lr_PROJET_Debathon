<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.web.*?>

<TitledPane fx:id="titledCreate" alignment="CENTER" animated="false" collapsible="false" text="titledCreate" textAlignment="CENTER" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.univlr.debathon.application.view.mainwindow.CreateDebateView">
    <font>
        <Font name="Calibri" size="14.0" />
    </font>
    <BorderPane>
        <bottom>
            <Button fx:id="btnValid" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#act_btnValid" text="btnValid" BorderPane.alignment="CENTER">
                <font>
                    <Font name="Calibri" size="13.0" />
                </font>
            </Button>
        </bottom>
        <center>
            <VBox alignment="CENTER" spacing="20.0" BorderPane.alignment="CENTER">
                <BorderPane.margin>
                    <Insets bottom="15.0" left="10.0" right="10.0" top="10.0" />
                </BorderPane.margin>
                <VBox maxWidth="1.7976931348623157E308" spacing="5.0">
                    <Label fx:id="lblTitle" maxWidth="1.7976931348623157E308" text="lblTitle">
                        <font>
                            <Font name="Calibri" size="13.0" />
                        </font>
                    </Label>
                    <TextField fx:id="tfTitle" maxWidth="1.7976931348623157E308">
                        <VBox.margin>
                            <Insets left="10.0" />
                        </VBox.margin>
                    </TextField>
                </VBox>
                <HBox maxWidth="1.7976931348623157E308" style="-fx-border-color: DARKBLUE; -fx-border-radius: 5;" BorderPane.alignment="CENTER">
                    <FlowPane fx:id="flowCategory" prefWidth="0.0" HBox.hgrow="NEVER">
                        <HBox.margin>
                            <Insets bottom="2.0" left="3.0" right="3.0" top="4.0" />
                        </HBox.margin></FlowPane>
                    <Separator orientation="VERTICAL" />
                    <FlowPane fx:id="flowTag" maxWidth="1.7976931348623157E308" minWidth="100.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
                        <HBox.margin>
                            <Insets bottom="2.0" left="3.0" right="3.0" top="4.0" />
                        </HBox.margin>
                    </FlowPane>
                    <Button fx:id="btnAddItem" maxHeight="1.7976931348623157E308" mnemonicParsing="false" onAction="#act_btnAddItem" style="-fx-background-radius: 5;" text="btnAddItem">
                        <HBox.margin>
                            <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                        </HBox.margin>
                    </Button>
               <VBox.margin>
                  <Insets left="10.0" />
               </VBox.margin>
                </HBox>
                <VBox maxWidth="1.7976931348623157E308" spacing="5.0">
                    <Label fx:id="lblDescription" maxWidth="1.7976931348623157E308" text="lblDescription">
                        <font>
                            <Font name="Calibri" size="13.0" />
                        </font>
                    </Label>
                    <HTMLEditor fx:id="htmlEditorDescription" maxHeight="400.0" maxWidth="550.0">
                        <VBox.margin>
                            <Insets left="10.0" />
                        </VBox.margin>
                    </HTMLEditor>
                </VBox>
                <VBox layoutX="10.0" layoutY="11.0" maxWidth="1.7976931348623157E308" spacing="5.0">
                    <Label fx:id="lblKey" maxWidth="1.7976931348623157E308" text="lblKey">
                        <font>
                            <Font name="Calibri" size="13.0"/>
                        </font>
                    </Label>
                    <TextField fx:id="tfKey" maxWidth="1.7976931348623157E308">
                        <VBox.margin>
                            <Insets left="10.0"/>
                        </VBox.margin>
                    </TextField>
                </VBox>
            </VBox>
        </center>
    </BorderPane>
</TitledPane>
