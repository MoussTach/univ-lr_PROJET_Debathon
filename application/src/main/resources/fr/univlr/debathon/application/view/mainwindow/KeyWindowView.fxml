<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane fx:id="borderPane_key" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.univlr.debathon.application.view.mainwindow.KeyWindowView">
    <center>
        <VBox spacing="10.0" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </BorderPane.margin>
            <Label fx:id="lblKey" maxWidth="1.7976931348623157E308" text="lblKey" VBox.vgrow="ALWAYS">
                <font>
                    <Font name="Calibri" size="14.0"/>
                </font>
            </Label>
            <TextField fx:id="tfKey" minWidth="350.0" prefHeight="30.0" prefWidth="350.0" VBox.vgrow="ALWAYS">
                <VBox.margin>
                    <Insets bottom="5.0" left="10.0" right="10.0" top="5.0"/>
                </VBox.margin>
                <font>
                    <Font name="Calibri" size="14.0"/>
                </font>
            </TextField>
        </VBox>
    </center>
    <bottom>
        <VBox BorderPane.alignment="CENTER">
            <Separator prefWidth="200.0"/>
            <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="200.0">
                <Button fx:id="btnValidKey" mnemonicParsing="false" onAction="#act_btnValidKey" prefWidth="75.0"
                        style="-fx-background-radius: 10;">
                    <graphic>
                        <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                            <Image url="@../../../../../../img/confirm_64.png"/>
                        </ImageView>
                    </graphic>
                </Button>
            </ToolBar>
        </VBox>
    </bottom>
</BorderPane>
