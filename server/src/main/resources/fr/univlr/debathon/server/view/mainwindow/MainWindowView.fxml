<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane fx:id="bPaneMainView" minHeight="100.0" minWidth="300.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.univlr.debathon.server.view.mainwindow.MainWindowView">
    <center>
        <VBox alignment="CENTER" spacing="20.0" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets bottom="15.0" left="10.0" right="10.0" top="10.0" />
            </BorderPane.margin>
            <VBox maxWidth="1.7976931348623157E308" spacing="5.0">
                <Label fx:id="lblTitle" maxWidth="1.7976931348623157E308" text="Key to grant the creation rights">
                    <font>
                        <Font name="Calibri" size="13.0" />
                    </font>
                </Label>
                <TextField fx:id="tfKey" alignment="CENTER" editable="false" maxWidth="1.7976931348623157E308">
                    <VBox.margin>
                        <Insets left="10.0" />
                    </VBox.margin>
               <font>
                  <Font name="Calibri" size="16.0" />
               </font>
                </TextField>
            </VBox>
        </VBox>
    </center>
</BorderPane>
